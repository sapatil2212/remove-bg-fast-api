{
  "version": 2,
  "builds": [
    {
      "src": "api/rembg.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/rembg/(.*)",
      "dest": "/api/rembg"
    },
    {
      "src": "/(.*)",
      "dest": "/api/rembg"
    }
  ],
  "functions": {
    "api/rembg.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  },
  "env": {
    "PYTHONPATH": "/var/task"
  }
}